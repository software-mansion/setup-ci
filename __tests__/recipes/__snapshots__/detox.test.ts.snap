  lookup-cached-build:
    runs-on: ubuntu-latest
    outputs:
      build-exists: \${{ steps.android-release-restore.outputs.cache-hit }}
    steps:
      - name: üêõ Try to restore build from cache
        id: android-release-restore
        uses: actions/cache/restore@v4
        with:
          lookup-only: true
          path: android-release-build/
          key: android-release-build-\${{ github.event.pull_request.head.sha }}

  build-release-android:
    needs: lookup-cached-build
    if: needs.lookup-cached-build.outputs.build-exists != 'true'
    runs-on: ubuntu-latest
    steps:
      - name: üíæ Maximize build space
        uses: AdityaGarg8/remove-unwanted-software@v4.1
        with:
          remove-dotnet: 'true'
          remove-haskell: 'true'
          remove-codeql: 'true'
          remove-docker-images: 'true'

      - name: üèó Checkout repository
        uses: actions/checkout@v4

      - name: üåø Setup Node
        uses: actions/setup-node@v4
        with:
          node-version-file: 'apps/expo-app/.nvmrc'
          cache: 'npm'

      - name: üì¶ Install dependencies
        run: npm ci

      - name: ‚òï Set up JDK 17
        uses: actions/setup-java@v4
        with:
          java-version: '17'
          distribution: 'zulu'

      - name: üêò Setup Gradle 8.8
        uses: gradle/actions/setup-gradle@v4
        with:
          gradle-version: 8.8

      - name: üõ†Ô∏è Build
        run: npm run build:release:android
        working-directory: apps/expo-app

