<%-
  (() => {
    switch (props.packageManager) {
      case 'npm':
        return 'npm ci';
      case 'yarn':
        return 'yarn install --immutable';
      case 'bun':
        return 'bun install --frozen-lockfile';
      case 'pnpm':
        return 'pnpm install --frozen-lockfile';
    }
  })()
%>
