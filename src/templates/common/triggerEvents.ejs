on:
<%= (props.pull_request) ? `
  pull_request: ${(props.isMonorepo) ? `
    paths:
      - ${props.pathRelativeToRoot}/**` : "" 
  }` : "" %> <%= (props.push) ? `
  push:
    branches: 
      - ${props.push} ${(props.isMonorepo) ? `
    paths:
      - ${props.pathRelativeToRoot}/**` : "" 
  }` : ""
%>
